{
  "private": false,
  "name": "gugis-sanity-frontend",
  "version": "1.0.0",
  "author": "Mantas Mikalauskas <mikalauskas.it@gmail.com>",
  "scripts": {
    "build": "npm run build:dir && npm run build:favicons && gatsby build",
    "develop": "gatsby develop",
    "lint": "eslint .",
    "build:dir": "mkdir -p public/icons/",
    "build:dir-win": "mkdir public\\icons\\",
    "build:favicons": "node scripts/favicons",
    "now-build": "npm run build",
    "start": "npm run develop",
    "format": "prettier-eslint --write \"**/*.js\" \"!.cache/**\" \"!node_modules/**\" \"!public/**\"",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@sanity/block-content-to-react": "^2.0.6",
    "@sanity/image-url": "^0.140.12",
    "date-fns": "^1.30.1",
    "dotenv": "^7.0.0",
    "gatsby": "^2.3.4",
    "gatsby-image": "^2.0.39",
    "gatsby-plugin-manifest": "^2.0.29",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-postcss": "^2.0.7",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-sharp": "^2.0.35",
    "gatsby-plugin-sitemap": "^2.0.12",
    "gatsby-source-sanity": "^2.0.2",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "favicons": "^5.3.0",
    "prettier-eslint-cli": "^4.7.1"
  }
}
